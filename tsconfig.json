{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "ES6",
    "moduleResolution": "Node",
    "resolveJsonModule": true
  }
}
